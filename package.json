{
  "name": "mobile-backend",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --quiet --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "prebuild": "cp package.json dist/ || mkdir -p dist/ && cp package.json dist/",
    "build": "tsc",
    "start": "NODE_PATH=./dist node dist/index.js",
    "worker:dev": "ts-node-dev --quiet --respawn --transpile-only -r tsconfig-paths/register src/jobs/crawlWorker.ts & ts-node-dev --quiet --respawn --transpile-only -r tsconfig-paths/register src/jobs/suggestQuestionsWorker.ts & ts-node-dev --quiet --respawn --transpile-only -r tsconfig-paths/register src/jobs/processQuestionsWorker.ts",
    "worker:start": "node dist/jobs/crawlWorker.js & node dist/jobs/suggestQuestionsWorker.js & node dist/jobs/processQuestionsWorker.js",
    "migration:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -d src/config/database.ts",
    "migration:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d src/config/database.ts",
    "migration:revert": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert -d src/config/database.ts",
    "seed:engines": "ts-node -r tsconfig-paths/register src/scripts/seedEngines.ts",
    "test:brand-extraction": "ts-node -r tsconfig-paths/register src/scripts/testBrandExtraction.ts",
    "test:competitor-analysis": "ts-node -r tsconfig-paths/register src/scripts/testCompetitorAnalysis.ts",
    "debug:brand-tracking": "ts-node -r tsconfig-paths/register src/scripts/debugBrandTracking.ts",
    "retroactive:brand-tracking": "ts-node -r tsconfig-paths/register src/scripts/retroactiveBrandTracking.ts",
    "check:llms-txt": "ts-node -r tsconfig-paths/register src/scripts/checkExistingLlmsTxt.ts",
    "test:llms-generation": "ts-node -r tsconfig-paths/register src/scripts/testLlmsTxtGeneration.ts"
  },
  "dependencies": {
    "@mendable/firecrawl-js": "^1.29.3",
    "@openrouter/ai-sdk-provider": "^1.1.2",
    "@supabase/supabase-js": "^2.54.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express-session": "^1.18.1",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/redis": "^4.0.10",
    "apisauce": "^3.2.0",
    "bcrypt": "^5.1.1",
    "body-parser": "^2.2.0",
    "bullmq": "^5.56.10",
    "class-validator": "^0.14.1",
    "connect-redis": "^6.1.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "express-session": "^1.18.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "node-cron": "^4.2.1",
    "openai": "^5.12.2",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "passport-spotify": "^2.0.0",
    "pg": "^8.14.1",
    "pgvector": "^0.2.1",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.22",
    "typeorm-naming-strategies": "^4.1.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^22.14.0",
    "nodemon": "^3.1.9",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}
